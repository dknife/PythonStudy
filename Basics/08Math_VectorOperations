import numpy as np

def printProducts(x, y) :
    print(x, " dot ", y, " = ", np.dot(x, y))
    print(x, " vdot ", y, " = ", np.vdot(x, y))
    print(x, " inner ", y, " = ", np.inner(x, y))
    print(x, " outer ", y, " = ", np.outer(x, y))


#scalar dot
a = 3
b = 4
printProducts(a,b)

a = [1,2,3]
b = [1,2,3]
printProducts(a,b)

a = 2 + 6j
b = 3 + 2j
printProducts(a,b)

a = [2 + 6j, 1+3j]
b = [3 + 2j, 3+1j]
printProducts(a,b)

a = [[1,1], [2,2]]
b = [[3,3], [4,4]]
printProducts(a,b)

