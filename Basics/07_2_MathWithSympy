from sympy import Symbol, factor, pprint, solve

x = Symbol('x')
y = Symbol('y')
expression = x**3 - 2*x**2*y - 2 * x*y**2 + x*y**2 + x**2 * y + y**3

print (expression, " = ", factor(expression))

print ("")

pprint (expression)
pprint (" = ")
pprint (factor(expression))

### Solve equation

print (solve(expression))

expression = x**2 + 5*x + 4
pprint (solve(expression))
